<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"> //set background 
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
        </shape>
        <color android:color="@color/parking_progress_bar_bg_color_night"/>
    </item> // set process 
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#2f3b48" android:endColor="#0698ff" android:angle="0" android:type="linear"/>
            </shape>
        </clip>
    </item>
</layer-list>
